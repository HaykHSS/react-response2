{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "RUN go install (?<depName>github\\.com/[^@]+)@(?<currentValue>[^\\s]+)"
      ],
      "depNameTemplate": "{{depName | replace('/cmd/swag', '') | replace('/v2', '')}}",
      "versioningTemplate": "go",
      "datasourceTemplate": "github-tags"
    }
  ]
}
